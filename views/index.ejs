      <form id="form" runat="server">
        <%- include('./partials/header'); %>
        <div class="center-block">
          <div class="container">
            <div class="row">
              <div class="col-md-12">
                <h1>Search</h1>
              </div>
              <div class="col-md-12">
                <div class="input-group">
                  <div class="wrapper">
                    <input type="text" class="form-control input-lg" placeholder="1.1.1.1" title="IP Address" />
                    <div class="controls">
                      <button>Search</button>
                    </div>
                  </div>
                </div>
              </div>
            </div>
            <div class="result hide">
              <div class="row">
                  <div class="col-md-12">
                    <h1>Result</h1>
                    <pre></pre>
                  </div>
              </div>
            </div>
          </div>
        </div>
        <%- include('./partials/footnote'); %>
      </form>
    </body>
    <%- include('./partials/footer'); %>
</html>